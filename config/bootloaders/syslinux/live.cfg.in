label live-@FLAVOUR@
	menu label ^Start or Install Tanglu (@FLAVOUR@)
	menu default
	linux @LINUX@
	initrd @INITRD@
	append @APPEND_LIVE@

label live-@FLAVOUR@-failsafe
	menu label Start or Install Tanglu (@FLAVOUR@ ^failsafe)
	linux @LINUX@
	initrd @INITRD@
	append @APPEND_LIVE_FAILSAFE@

label hd
	menu label ^Boot from first hard disk
	localboot 0x80
	append -
